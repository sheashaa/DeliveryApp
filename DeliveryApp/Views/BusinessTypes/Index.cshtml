@model IEnumerable<DeliveryApp.Models.BusinessType>

@{
    ViewBag.Title = "Index";
}

<h2>Business Type</h2>

<p>
    @Html.ActionLink("Create New Business Type", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BusinessTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BusinessTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BusinessTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.BusinessTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.BusinessTypeId })
        </td>
    </tr>
}

</table>
